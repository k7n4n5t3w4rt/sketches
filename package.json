{
  "name": "es-module-preact-boilerplate",
  "description": "For making new Preact projects with server-side rendering, es modules and NO compilation during development",
  "version": "1.0.0",
  "author": "Kynan Stewart",
  "bugs": {
    "url": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate/issues"
  },
  "dependencies": {
    "finalhandler": "^1.1.2",
    "history": "^4.10.1",
    "htm": "^3.0.4",
    "preact": "^10.4.1",
    "preact-render-to-string": "^5.1.6",
    "preact-router": "^3.2.1",
    "screenfull": "^5.0.2",
    "serve-static": "^1.14.1",
    "simplestyle-js": "github:k7n4n5t3w4rt/simplestyle#dist",
    "three": "^0.116.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "faucet": "0.0.1",
    "fixpack": "^3.0.6",
    "flow-bin": "^0.123.0",
    "glob": "^7.1.6",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "should": "^13.2.3",
    "snowpack": "^1.7.1"
  },
  "homepage": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate#readme",
  "keywords": [
    "boilerplate",
    "es",
    "modules"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate.git"
  },
  "scripts": {
    "browsersync": "browser-sync start --server --files \"./\"",
    "build": "node --no-warnings server/static_generate.js",
    "fixpack": "fixpack",
    "flow": "flow check",
    "snowpack": "snowpack install --clean && grep -r -l _commonjsHelpers web_modules | xargs sed -i '' s/_commonjsHelpers/commonjshelpers/g $i && mv web_modules/common/_commonjsHelpers-6a48b99e.js web_modules/common/commonjshelpers-6a48b99e.js",
    "start": "node server/index.js",
    "test": "node --no-warnings server/testyrunner.js | faucet",
    "unbuild": "node --no-warnings server/static_clear.js"
  },
  "snowpack": {
    "webDependencies": [
      "history",
      "htm",
      "preact",
      "preact/hooks",
      "preact-render-to-string",
      "preact-router",
      "screenfull",
      "should/as-function.js",
      "simplestyle-js",
      "three"
    ],
    "dedupe": []
  },
  "type": "module"
}
